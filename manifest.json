{
    "manifest_version": 3,
    "name": "Cookie Logger",
    "version": "1.0.0",
    "action": {},
    "background": {
        "service_worker": "src/background/index.js",
        "type": "module"
    },
    "permissions": ["scripting", "activeTab", "tabs", "cookies", "webRequest"],
    "host_permissions": ["https://*/*", "http://*/*"]
}